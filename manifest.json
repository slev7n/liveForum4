{
	"manifest_version": 2,
	"name": "LiveForum",
	"version": "4.0",
	"description": "Giving a glass of ice water to forum.ge users",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},

	"content_scripts": [
		{
			"matches": ["*://forum.ge/*", "*://*.forum.ge/*"],
			"js": ["js/main.js"],
			"css": ["css/main.css"]
		}
	]
}